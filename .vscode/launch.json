    "version": "0.2.0",
    "configurations": [
        {
            "name": "Initialize",
            "type": "pythonExperimental",
            "request": "launch",
            "program": "${workspaceFolder}/puckdb/console.py",
            "args": [
                "init"
            ],
            "console": "integratedTerminal",
            "envFile": "."
        },
        {
            "name": "Get",
            "type": "pythonExperimental",
            "request": "launch",
            "program": "${workspaceFolder}/puckdb/console.py",
            "args": [
                "get",
                "--to-date",
                "2017-07-01"
            ],
            "console": "integratedTerminal",
            "envFile": "."
        }
    ]
}